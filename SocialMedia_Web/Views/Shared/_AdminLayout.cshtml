@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard - Mazer Admin Dashboard</title>



    <link rel="shortcut icon" href="~/admin/assets/compiled/svg/favicon.svg" type="image/x-icon">
    <link rel="shortcut icon" href="data:image/png;base64," type="image/png">

    @await RenderSectionAsync("css", required: false)
    <link rel="stylesheet" href="~/admin/assets/extensions/simple-datatables/style.css">
    <link rel="stylesheet" href="~/admin/assets/compiled/css/table-datatable.css">
    <link rel="stylesheet" href="~/admin/assets/extensions/toastify-js/src/toastify.css">
    <link rel="stylesheet" href="~/admin/assets/compiled/css/app.css">
    <link rel="stylesheet" href="~/admin/assets/compiled/css/app-dark.css">
    <link rel="stylesheet" href="~/admin/assets/compiled/css/iconly.css">
</head>

<body>
    <script src="~/admin/~/admin/assets/static/js/initTheme.js"></script>
    <div id="app">
        <div id="sidebar">
            <div class="sidebar-wrapper active">
                <div class="sidebar-header position-relative">
                    <div class="d-flex justify-content-between align-items-center">
                        <div class="logo">
                            <a href="/Admin/Home/Index">
                                Admin
                                @* <img src="~/admin/assets/compiled/svg/logo.svg" alt="Logo" srcset=""> *@
                            </a>
                        </div>
                        <div class="theme-toggle d-flex gap-2  align-items-center mt-2">
                            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true"
                                 role="img" class="iconify iconify--system-uicons" width="20" height="20"
                                 preserveAspectRatio="xMidYMid meet" viewBox="0 0 21 21">
                                <g fill="none" fill-rule="evenodd" stroke="currentColor" stroke-linecap="round"
                                   stroke-linejoin="round">
                                    <path d="M10.5 14.5c2.219 0 4-1.763 4-3.982a4.003 4.003 0 0 0-4-4.018c-2.219 0-4 1.781-4 4c0 2.219 1.781 4 4 4zM4.136 4.136L5.55 5.55m9.9 9.9l1.414 1.414M1.5 10.5h2m14 0h2M4.135 16.863L5.55 15.45m9.899-9.9l1.414-1.415M10.5 19.5v-2m0-14v-2"
                                          opacity=".3"></path>
                                    <g transform="translate(-210 -1)">
                                        <path d="M220.5 2.5v2m6.5.5l-1.5 1.5"></path>
                                        <circle cx="220.5" cy="11.5" r="4"></circle>
                                        <path d="m214 5l1.5 1.5m5 14v-2m6.5-.5l-1.5-1.5M214 18l1.5-1.5m-4-5h2m14 0h2"></path>
                                    </g>
                                </g>
                            </svg>
                            <div class="form-check form-switch fs-6">
                                <input class="form-check-input  me-0" type="checkbox" id="toggle-dark" style="cursor: pointer">
                                <label class="form-check-label"></label>
                            </div>
                            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true"
                                 role="img" class="iconify iconify--mdi" width="20" height="20" preserveAspectRatio="xMidYMid meet"
                                 viewBox="0 0 24 24">
                                <path fill="currentColor"
                                      d="m17.75 4.09l-2.53 1.94l.91 3.06l-2.63-1.81l-2.63 1.81l.91-3.06l-2.53-1.94L12.44 4l1.06-3l1.06 3l3.19.09m3.5 6.91l-1.64 1.25l.59 1.98l-1.7-1.17l-1.7 1.17l.59-1.98L15.75 11l2.06-.05L18.5 9l.69 1.95l2.06.05m-2.28 4.95c.83-.08 1.72 1.1 1.19 1.85c-.32.45-.66.87-1.08 1.27C15.17 23 8.84 23 4.94 19.07c-3.91-3.9-3.91-10.24 0-14.14c.4-.4.82-.76 1.27-1.08c.75-.53 1.93.36 1.85 1.19c-.27 2.86.69 5.83 2.89 8.02a9.96 9.96 0 0 0 8.02 2.89m-1.64 2.02a12.08 12.08 0 0 1-7.8-3.47c-2.17-2.19-3.33-5-3.49-7.82c-2.81 3.14-2.7 7.96.31 10.98c3.02 3.01 7.84 3.12 10.98.31Z">
                                </path>
                            </svg>
                        </div>
                        <div class="sidebar-toggler  x">
                            <a href="#" class="sidebar-hide d-xl-none d-block"><i class="bi bi-x bi-middle"></i></a>
                        </div>
                    </div>
                </div>
                <div class="sidebar-menu">
                    <ul class="menu">

                        <li class="sidebar-item  ">
                            <a href="/Admin/Home/Index" class='sidebar-link'>
                                <i class="iconly-boldBookmark"></i>
                                <span>Paylaşımlar</span>
                            </a>
                        </li>

                        <li class="sidebar-item  ">
                            <a href="/Admin/Topic/Index" class='sidebar-link'>
                                <i class="iconly-boldDocument"></i>
                                <span>Başlıklar</span>
                            </a>
                        </li>

                        <li class="sidebar-item  ">
                            <a href="/Admin/User/Index" class='sidebar-link'>
                                <i class="iconly-boldProfile"></i>
                                <span>Kullanıcılar</span>
                            </a>
                        </li>

                        <li class="sidebar-item  ">
                            <a href="/Admin/Claim/Index" class='sidebar-link'>
                                <i class="bi bi-shield-lock-fill"></i>
                                <span>Yetkiler</span>
                            </a>
                        </li>

                        @* <li class="sidebar-item  ">
                            <a href="/Admin/Message/Index" class='sidebar-link'>
                                <i class="bi bi-envelope-at-fill"></i>
                                <span>Mesajlar</span>
                                <span class="badge bg-success badge-pill badge-round ms-3">5</span>
                            </a>
                        </li> *@

                    </ul>
                </div>
            </div>
        </div>
        <div id="main">
            <header class="mb-3">
                <a href="#" class="burger-btn d-block d-xl-none">
                    <i class="bi bi-justify fs-3"></i>
                </a>
            </header>

            @* <div class="page-heading">
                <h3>Profile Statistics</h3>
            </div> *@
            <div class="page-content">
                <section class="row">
                    <div class="col-12 col-lg-9">
                        <div class="row d-flex justify-content-center">
                            <div class="col-6 col-lg-3 col-md-6">
                                <div class="card">
                                    <div class="card-body px-4 py-4-5">
                                        <div class="row">
                                            <div class="col-md-4 col-lg-12 col-xl-12 col-xxl-5 d-flex justify-content-start ">
                                                <div class="stats-icon purple mb-2">
                                                    <i class="iconly-boldDocument"></i>
                                                </div>
                                            </div>
                                            <div class="col-md-8 col-lg-12 col-xl-12 col-xxl-7">
                                                <a href="/Admin/Topic/Index">
                                                    <h6 class="text-muted font-semibold">Başlıklar</h6>
                                                </a>
                                                <h6 class="font-extrabold mb-0">112.000</h6>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-6 col-lg-3 col-md-6">
                                <div class="card">
                                    <div class="card-body px-4 py-4-5">
                                        <div class="row">
                                            <div class="col-md-4 col-lg-12 col-xl-12 col-xxl-5 d-flex justify-content-start ">
                                                <div class="stats-icon blue mb-2">
                                                    <i class="iconly-boldProfile"></i>
                                                </div>
                                            </div>
                                            <div class="col-md-8 col-lg-12 col-xl-12 col-xxl-7">
                                                <a href="/Admin/User/Index">
                                                    <h6 class="text-muted font-semibold">Kullanıcılar</h6>
                                               </a>
                                                <h6 class="font-extrabold mb-0">183.000</h6>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            
                            <div class="col-6 col-lg-3 col-md-6">
                                <div class="card">
                                    <div class="card-body px-4 py-4-5">
                                        <div class="row">
                                            <div class="col-md-4 col-lg-12 col-xl-12 col-xxl-5 d-flex justify-content-start ">
                                                <div class="stats-icon red mb-2">
                                                    <i class="iconly-boldBookmark"></i>
                                                </div>
                                            </div>
                                            <div class="col-md-8 col-lg-12 col-xl-12 col-xxl-7">
                                                <a href="/Admin/Home/Index">
                                                    <h6 class="text-muted font-semibold">Paylaşımlar</h6>
                                               </a>
                                                <h6 class="font-extrabold mb-0">112</h6>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            
                                @RenderBody()
                            
                        </div>
                       
                    </div>
                    <div class="col-12 col-lg-3">
                        <div class="card">
                            <div class="card-body py-4 px-4">
                                <div class="d-flex align-items-center">
                                    <div class="avatar avatar-xl">
                                        <img src="~/admin/assets/compiled/jpg/1.jpg" alt="Face 1">
                                    </div>
                                    <div class="ms-3 name">
                                        <h5 class="font-bold">John Duck</h5>
                                        <h6 class="text-muted mb-0">@@johnducky</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card">
                            <div class="card-header">
                                <h4>Son Mesajlar</h4>
                            </div>
                            <div class="card-content pb-4">
                                <div class="recent-message d-flex px-4 py-3">
                                    <div class="avatar avatar-lg">
                                        <img src="~/admin/assets/compiled/jpg/4.jpg">
                                    </div>
                                    <div class="name ms-4">
                                        <h5 class="mb-1">Hank Schrader</h5>
                                        <h6 class="text-muted mb-0">@@johnducky</h6>
                                    </div>
                                </div>
                                <div class="recent-message d-flex px-4 py-3">
                                    <div class="avatar avatar-lg">
                                        <img src="~/admin/assets/compiled/jpg/5.jpg">
                                    </div>
                                    <div class="name ms-4">
                                        <h5 class="mb-1">Dean Winchester</h5>
                                        <h6 class="text-muted mb-0">@@imdean</h6>
                                    </div>
                                </div>
                                <div class="recent-message d-flex px-4 py-3">
                                    <div class="avatar avatar-lg">
                                        <img src="~/admin/assets/compiled/jpg/1.jpg">
                                    </div>
                                    <div class="name ms-4">
                                        <h5 class="mb-1">John Dodol</h5>
                                        <h6 class="text-muted mb-0">@@dodoljohn</h6>
                                    </div>
                                </div>
                                <div class="px-4">
                                    <button class='btn btn-block btn-xl btn-outline-primary font-bold mt-3'>Start Conversation</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
           
            <footer>
                <div class="footer clearfix mb-0 text-muted">
                    <div class="float-start">
                        <p>2024 &copy; Sosyal Medya Web Sitesi</p>
                    </div>
                    @* <div class="float-end">
                        <p>
                            Sosyal Medya Web Sitesi <span class="text-danger"><i class="bi bi-heart-fill icon-mid"></i></span>
                            by <a href="https://saugi.me">Admin Paneli</a>
                        </p>
                    </div> *@
                </div>
            </footer>
        </div>
    </div>
    <script src="~/admin/assets/static/js/components/dark.js"></script>
    <script src="~/admin/assets/extensions/perfect-scrollbar/perfect-scrollbar.min.js"></script>


    <script src="~/admin/assets/compiled/js/app.js"></script>



    <!-- Need: Apexcharts -->
    <script src="~/admin/assets/extensions/apexcharts/apexcharts.min.js"></script>
    <script src="~/admin/assets/static/js/pages/dashboard.js"></script>

    <!--Simple Datatable-->
    <script src="~/admin/assets/extensions/simple-datatables/umd/simple-datatables.js"></script>
    <script src="~/admin/assets/static/js/pages/simple-datatables.js"></script>

    <script src="~/admin/assets/extensions/jquery/jquery.min.js"></script>
    <script src="~/admin/assets/extensions/toastify-js/src/toastify.js"></script>
    @await RenderSectionAsync("Scripts", required: false)

</body>

</html>